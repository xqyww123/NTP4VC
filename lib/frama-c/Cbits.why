  axiom is_uint64_land : forall x y:int [Cint.to_uint64 (land x y)].
    Cint.is_uint64 x -> Cint.is_uint64 y -> Cint.to_uint64 (land x y) = land x y

  axiom is_uint64_lsr : forall x y:int [Cint.to_uint64 (lsr x y)].
    0<=y -> Cint.is_uint64 x -> Cint.to_uint64 (lsr x y) = lsr x y

  axiom is_uint64_lsl1_inf : forall y:int [Cint.to_uint64 (lsl 1 y)].
    0<=y<64 -> Cint.to_uint64 (lsl 1 y) = lsl 1 y

  axiom is_uint64_lsl1_sup : forall y:int [Cint.to_uint64 (lsl 1 y)].
    64<=y -> Cint.to_uint64 (lsl 1 y) = 0

(** ** Signed conversions *)
  lemma is_sint_lnot: forall n x:int.
    Cint.is_sint n x -> Cint.to_sint n (lnot x) = lnot x
  lemma is_sint_lxor: forall n x y:int.
    Cint.is_sint n x -> Cint.is_sint n y -> Cint.to_sint n (lxor x y) = lxor x y
  lemma is_sint_lor: forall n x y:int.
    Cint.is_sint n x -> Cint.is_sint n y -> Cint.to_sint n (lor x y) = lor x y
  lemma is_sint_land: forall n x y:int.
    Cint.is_sint n x -> Cint.is_sint n y -> Cint.to_sint n (land x y) = land x y
  lemma is_sint_lsr: forall n x y:int.
    0<=y -> Cint.is_sint n x -> Cint.to_sint n (lsr x y) = lsr x y
  lemma is_sint_lsl1_inf : forall n y:int.
    0<=y<n -> Cint.to_sint n (lsl 1 y) = (lsl 1 y)
  lemma is_sint_lsl1_sup : forall n y:int.
    0<=n<y -> Cint.to_sint n (lsl 1 y) = 0

