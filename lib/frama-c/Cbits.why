(** * Some C-Integer Bits Conversions are distributive *)
(** ** Unsigned conversions *)
  lemma to_uint_lor : forall n x y:int.
    Cint.to_uint n (lor x y) = lor (Cint.to_uint n x) (Cint.to_uint n y)

(** *** Cast to uint8 C type *)
  lemma to_uint8_lor : forall x y:int [Cint.to_uint8 (lor x y)].
    Cint.to_uint8 (lor x y) = lor (Cint.to_uint8 x) (Cint.to_uint 8 y)

(** ***  Cast to uint16 C type *)
  lemma to_uint16_lor : forall x y:int [Cint.to_uint16 (lor x y)].
    Cint.to_uint16 (lor x y) = lor (Cint.to_uint16 x) (Cint.to_uint16 y)

(** ***  Cast to uint32 C type *)
  axiom to_uint32_lor : forall x y:int [Cint.to_uint32 (lor x y)].
    Cint.to_uint32 (lor x y) = lor (Cint.to_uint32 x) (Cint.to_uint32 y)

(** ***  Cast to uint64 C type *)
  lemma to_uint64_lor : forall x y:int [Cint.to_uint64 (lor x y)].
    Cint.to_uint64 (lor x y) = lor (Cint.to_uint64 x) (Cint.to_uint64 y)

(** * Some C-Integer Bits Conversions are identity *)
(** ** Unsigned conversions *)
  lemma is_uint_lxor : forall n x y:int.
    Cint.is_uint n x -> Cint.is_uint n y -> Cint.to_uint n (lxor x y) = lxor x y
  lemma is_uint_lor : forall n x y:int.
    Cint.is_uint n x -> Cint.is_uint n y -> Cint.to_uint n (lor x y) = lor x y
  lemma is_uint_land : forall n x y:int.
    Cint.is_uint n x -> Cint.is_uint n y -> Cint.to_uint n (land x y) = land x y
  lemma is_uint_lsr : forall n x y:int.
    0<=y -> Cint.is_uint n x -> Cint.to_uint n (lsr x y) = lsr x y
  lemma is_uint_lsl1_inf : forall n y:int.
    0<=y<n  -> Cint.to_uint n (lsl 1 y) = lsl 1 y
  lemma is_uint_lsl1_sup : forall n y:int.
    0<=n<=y -> Cint.to_uint n (lsl 1 y) = 0

(** *** Cast to uint8 C type *)
  axiom is_uint8_lxor : forall x y:int [Cint.to_uint8 (lxor x y)].
    Cint.is_uint8 x -> Cint.is_uint8 y -> Cint.to_uint8 (lxor x y) = lxor x y

  axiom is_uint8_lor : forall x y:int [Cint.to_uint8 (lor x y)].
    Cint.is_uint8 x -> Cint.is_uint8 y -> Cint.to_uint8 (lor x y) = lor x y

